<Window x:Class="прокат.авто.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:прокат.авто"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Background="Aqua"></Grid>
        <Image Source="/Resources/car.jpg" HorizontalAlignment="Left" Width="213" Margin="23,0,0,0"/>
        <TextBlock Text="Прокат автомобиля" Foreground="Black" FontSize="30" HorizontalAlignment="Left" VerticalAlignment="Center" Width="331" Margin="272,0,0,0"/>
        <Frame Grid.Row="1" Name="MainFrame"></Frame>
    </Grid>
</Window>
